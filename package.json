{
    "name": "awap-back-end",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "db:up": "docker-compose -f \"docker-compose.yml\" up -d --build",
        "db:down": "docker-compose -f \"docker-compose.yml\" down -v",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^16.0.3",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "morgan": "~1.9.1",
        "pg": "^8.8.0"
    },
    "devDependencies": {
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.4.0",
        "eslint-plugin-promise": "^6.1.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1"
    }
}
